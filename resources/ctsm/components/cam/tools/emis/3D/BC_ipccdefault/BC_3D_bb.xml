<?xml version='1.0' encoding='us-ascii'?> 

<!-- SAMPLE INPUT SPECIFICATION FOR CAM-OSLO  --> 

<CamOsloInputConfiguration>

	<inputDataPath name="/disk1/alfg/noresm2emis/tmp"/>
	<outputDataPath name="/disk1/alfg/camOsloEmis"/>

   <!--THIS MERGES EMISSIONS FOR ANTHROPOGENIS AND SHIPS IN SAME FILES.. RECORDS ON SAME DATES WILL BE MERGED -->
   <!--NOTE: PROBLEM WITH DATES IN CURRENT SHIPPING FILE MAKES THIS OUTPUT QUITE STRANGE.. NEED TO FIX THAT -->
   <outputfile
      name="BC_bb_3D_default.nc"
      levelValues="100 , 500, 1000, 3000, 6000"
      year="1990,2000">
      <inputfile 
             name="BC_2D_Biomass_moleccm2.nc"
             levelValues="50, 300, 750, 1500, 2500, 4500">
             <fields>
                 <field name="grassfire" sector="grassfire"/>
                 <field name="forestfire" sector="forestfire"/>
             </fields>
             <levelFractionFile
                name="GFED_levelFractions_BC_2000.nc">
             </levelFractionFile>
      </inputfile>
   </outputfile>

</CamOsloInputConfiguration>
