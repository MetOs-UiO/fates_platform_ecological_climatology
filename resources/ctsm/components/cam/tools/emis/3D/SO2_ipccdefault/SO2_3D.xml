<?xml version='1.0' encoding='us-ascii'?> 


<CamOsloInputConfiguration>

	<inputDataPath name="/disk1/alfg/noresm2emis/tmp/"/>
	<outputDataPath name="/disk1/alfg/camOsloEmis"/>

   <!-- HISTORICAL EVOLUTION OF BC FOSSIL FUEL EMISSIONS  -->
   <outputfile
            name="SO2_3D_IPCC_1850_2005.nc"
            levelValues="150,500,1000,1600,3000,6000"
            >
      <inputfile 
               name="SO2_ship_2000.nc"
               year="2000">
         <fields>
            <!-- possible fields are emissionLevel=n, output=override.nc"-->
            <field name="emiss_shp" sector="ships" level="3"/>
         </fields>
      </inputfile>
      <inputfile 
               name="ECLIPSE_VOLC_1985_2050_vertical_HEIGHT.nc"
               levelValues="100,500,1000,5000,10000,20000" 
               year="2000">
         <fields>
            <!-- possible fields are emissionLevel=n, output=override.nc"-->
            <field name="SO2" sector="volcano"/>
         </fields>
      </inputfile>
   </outputfile>

</CamOsloInputConfiguration>
