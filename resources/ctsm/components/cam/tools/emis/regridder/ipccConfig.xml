<?xml version='1.0' encoding='us-ascii'?> 

<!-- SAMPLE INPUT SPECIFICATION FOR CAM-OSLO  --> 

<CamOsloInputConfiguration>

	<inputDataPath name="/disk1/alfg/linkToNorstoreEmissions"/>
	<outputDataPath name="/disk1/alfg/camOsloEmis"/>

	<outputfiles>

		<!--START COMPONENT BLACK CARBON -->

		<!-- HISTORICAL EVOLUTION OF BC FOSSIL FUEL EMISSIONS  -->
		<outputfile
					name="BC_ff_1850_2005.nc"
					field="emis_bc_ff"
					component="BC"
		  	  		provider="IPCC"
					scenario="historical"
					source="ff"
					>
			<inputfile 
	    	  			name="cmipemis/IPCC_emissions_BC_anthropogenic_$year$_0.5x0.5_v1_21_05_2009.nc"
		      			year="1850,1860,1870,1880,1890,1900,1910,1920,1930,1940,1950,1960,1970,1980,1990,2000">
				<fields>
					<field name="emiss_dom" sector="domestic" level="2"/>
					<field name="emiss_ene" sector="energy"/>
					<field name="emiss_ind" sector="industry"/>
					<field name="emiss_tra" sector="traffic"/>
					<field name="emiss_wst" sector="wasteburning"/>
					<field name="emiss_awb" sector="agriculturalwaste"/>
		  		</fields>
			</inputfile>
			<inputfile 
	    	  			name="cmipemis/IPCC_emissions_BC_ships_$year$_0.5x0.5_v1_20_04_2009.nc"
		      			year="1850,1860,1870,1880,1890,1900,1910,1920,1930,1940,1950,1960,1970,1980,1990,2000">
				<fields>
					<field name="emiss_shp" sector="ships"/>
		  		</fields>
			</inputfile>
		</outputfile>
		<!--Air craft tendencies in kg/m3/s are converted to fluxes in kg/m2/s by multiplying by height levels -->
		<outputfile
					name="BC_air_1850_2005.nc"
					field="emis_bc_ff"
					component="BC"
		  	  		provider="IPCC"
					scenario="historical"
					source="air"
					levels="26"
					levelValues="0.29 , 0.48 , 0.84 , 1.4 , 2.2 , 3.2 , 4.3 , 5.6 , 6.8 , 7.9 , 9.1 , 10.2 , 11.2 , 12.2, 13.2 ,14.2 , 15.2 , 16.2 , 17.1 , 18.3 , 20.0 , 22.3 , 25.12 , 28.7 , 32.9 , 37.9">
			<inputfile 
	    	  			name="cmipemis/IPCC_emissions_BC_aircraft_$year$_0.5x0.5_v1_23_07_2009.nc"
		      			year = "1850,1860,1870,1880,1890,1900,1910,1920,1930,1940,1950,1960,1970,1980,1990,2000">
				<fields>
		  			<!-- possible fields are emissionLevel=n, output=override.nc"-->
					<field name="emiss_air" sector="aircraft"/>
		  		</fields>
               <conversions>
                    <conversion type="multiplyByLayerHeight" reason="convert kg/m3/s ==>kg/m2/s"/>
                    <conversion type="convertToLayerMidpoints" reason="output values in layer midpoints"/>
                </conversions>
			</inputfile>

		</outputfile>

		<!-- HISTORICAL EVOLUTION OF BLACK CARBON (BB)  -->
		<outputfile
					name="BC_bb_1850_2005.nc"
					field="emis_bc_bb"
					component="BC"
					provider="IPCC"
					source = "bb"
					scenario="historical">
			<inputfile 
	    	  			name="cmipemis/IPCC_GriddedBiomassBurningEmissions_BC_decadalmonthlymean$year$_v1.nc"
						scenario="historical"
		      			year="1850,1860,1870,1880,1890,1900,1910,1920,1930,1940,1950,1960,1970,1980,1990,2000">
				<fields>
		  			<!-- possible fields are emissionLevel=n, output=override.nc"-->
					<field name="grassfire"  sector="grassfire"/>
					<field name="forestfire" sector="forestfire"/>
		  		</fields>
			</inputfile>
		</outputfile>

		<!-- RPC85 SCENARIO FOR BC FOSSIL FUEL  -->
		<outputfile
					name="BC_ff_RCP85.nc"
					field="emis_bc_ff"
					component="BC"
					provider="IPCC"
					source = "ff"
					scenario="RCP85"
					>
			<inputfile
					name="cmipemis/RCP85/IPCC_emissions_RCP85_BC_anthropogenic_$year$_0.5x0.5_v1_21_12_2009.nc"
					year="2005,2010,2020,2030,2040,2050,2060,2070,2080,2090,2100">
				<fields>
		  			<!-- possible fields are emissionLevel=n, output=override.nc"-->
					<field name="emiss_ene" sector="energy"/>
					<field name="emiss_dom" sector="domestic"/>
					<field name="emiss_ind" sector="industry"/>
					<field name="emiss_tra" sector="traffic"/>
					<field name="emiss_wst" sector="waste"/>
					<field name="emiss_agr" sector="agriculture"/>     <!--NOT IN HISTORICAL EMISIONS-->
					<field name="emiss_slv" sector="solvent"/>         <!--NOT IN HISTORICAL EMISSIONS-->
					<field name="emiss_awb" sector="agriculturWaste"/>
		  		</fields>
			</inputfile>
			<inputfile 
	    	  			name="cmipemis/RCP85/IPCC_emissions_RCP85_BC_ships_$year$_0.5x0.5_v1_21_12_2009.nc"
		      			year="2005,2010,2020,2030,2040,2050,2060,2070,2080,2090,2100">
				<fields>
					<field name="emiss_shp" sector="ships"/>
		  		</fields>
			</inputfile>
		</outputfile>
			<!--Air craft tendencies in kg/m3/s are converted to fluxes in kg/m2/s by multiplying by height levels -->
		<outputfile
					name="BC_air_RCP85.nc"
					field="emis_bc_ff"
					component="BC"
					provider="IPCC"
					scenario="RCP85"
					source="air"
					levels="26"
					levelValues="0.29 , 0.48 , 0.84 , 1.4 , 2.2 , 3.2 , 4.3 , 5.6 , 6.8 , 7.9 , 9.1 , 10.2 , 11.2 , 12.2, 13.2 ,14.2 , 15.2 , 16.2 , 17.1 , 18.3 , 20.0 , 22.3 , 25.12 , 28.7 , 32.9 , 37.9">
			<inputfile 
	    	  			name="cmipemis/RCP85/IPCC_emissions_RCP85_BC_aircraft_$year$_0.5x0.5_v1_21_12_2009.nc"
		      			year="2005,2010,2020,2030,2040,2050,2060,2070,2080,2090,2100">
				<fields>
		  			<!-- possible fields are emissionLevel=n, output=override.nc"-->
					<field name="emiss_air" sector="aircraft" conversion="multiplyByHeight"/>
		  		</fields>
                <conversions>
                    <conversion type="multiplyByLayerHeight" reason="convert kg/m3/s ==>kg/m2/s"/>
                    <conversion type="convertToLayerMidpoints" reason="output values in layer midpoints"/>
                </conversions>
			</inputfile>
		</outputfile>


		<!--RCP85 evolution of BC (BB) -->
		<outputfile
					name="BC_bb_RCP85.nc"
					field="emis_bc_bb"
					component="BC"
					provider="IPCC"
					source = "bb"
					scenario="RCP85">
			<inputfile 
	    	  			name="cmipemis/RCP85/IPCC_emissions_RCP85_BC_biomassburning_$year$_0.5x0.5_v1_21_12_2009.nc"
		      			year="2005,2010,2020,2030,2040,2050,2060,2080,2100">
				<fields>
		  			<!-- possible fields are emissionLevel=n, output=override.nc"-->
					<field name="grassfire"  sector="grassfire"/>
					<field name="forestfire" sector="forestfire"/>
		  		</fields>
			</inputfile>
		</outputfile>

		<!--START COMPONENT OC -->	
		<outputfile
					name="POM_ff_1850_2000.nc"
					field="emis_pom_ff"
					component="OC"
					provider="IPCC"
					source="ff"
					scenario="historical">
			<inputfile 
	    	  			name="cmipemis/IPCC_emissions_OC_anthropogenic_$year$_0.5x0.5_v1_21_05_2009.nc"
		      			year="1850,1860,1870,1880,1890,1900,1910,1920,1930,1940,1950,1960,1970,1980,1990,2000">
				<fields>
		  			<!-- possible fields are emissionLevel=n, output=override.nc"-->
					<field name="emiss_ene" sector="energy"/>
					<field name="emiss_dom" sector="domestic"/>
					<field name="emiss_ind" sector="industry"/>
					<field name="emiss_tra" sector="traffic"/>
					<field name="emiss_wst" sector="waste"/>
					<field name="emiss_awb" sector="agriculturWaste"/>
		  		</fields>
			</inputfile>
			<inputfile 
	    	  			name="cmipemis/IPCC_emissions_OC_ships_$year$_0.5x0.5_v1_20_04_2009.nc"
		      			year="1850,1860,1870,1880,1890,1900,1910,1920,1930,1940,1950,1960,1970,1980,1990,2000">
				<fields>
		  			<!-- possible fields are emissionLevel=n, output=override.nc"-->
					<field name="emiss_shp" sector="shipping"/>
		  		</fields>
			</inputfile>
		</outputfile>

		
		<outputfile
					name="POM_bb_1850_2010.nc"
					field="emis_pom_bb"
					component="OC"
					provider="IPCC"
					source = "bb"
					scenario="historical">
			<inputfile 
	    	  			name="cmipemis/IPCC_GriddedBiomassBurningEmissions_OC_decadalmonthlymean$year$_v1.nc"
		      			year="1850,1860,1870,1880,1890,1900,1910,1920,1930,1940,1950,1960,1970,1980,1990,2000">
				<fields>
		  			<!-- possible fields are emissionLevel=n, output=override.nc"-->
					<field name="grassfire"  sector="grassfire"/>
					<field name="forestfire" sector="forestfire"/>
		  		</fields>
			</inputfile>
		</outputfile>


		<!--START COMPONENT SO2 -->

		<!--HISTORICAL EVOLUTION OF SO2 FOSSIL FUEL EMISSIONS -->
		<outputfile
					name="IPCC_emiss_so2ff_1850-2005.nc"
					field="emis_SO2_ff"
					component="SO2"
					source = "ff"
					provider = "IPCC"
					scenario="historical">
			<inputfile 
	    	  			name="cmipemis/IPCC_emissions_SO2_anthropogenic_$year$_0.5x0.5_v1_13_05_2009.nc"
		  	  			provider="IPCC"
		      			year="1860,1860,1870,1880,1890,1900,1910,1920,1930,1940,1950,1960,1970,1980,1990">
					<fields>
		  				<!-- possible fields are emissionLevel=n, output=override.nc"-->
						<field name="emiss_dom" sector="domestic"/>
						<field name="emiss_ene" sector="energy"/>
						<field name="emiss_ind" sector="industry"/>
						<field name="emiss_tra" sector="traffic"/>
						<field name="emiss_wst" sector="wasteburning"/>
						<field name="emiss_awb" sector="agriculturalwaste"/>
		  			</fields>
			</inputfile>
			<!--Same as above, but different file version... One less sector no energy in this one -->
			<inputfile 
	    	  			name="cmipemis/IPCC_emissions_SO2_anthropogenic_$year$_0.5x0.5_v1_20_04_2009.nc"
		  	  			provider="IPCC"
		      			year="1850,2000" >
					<fields>
		  				<!-- possible fields are emissionLevel=n, output=override.nc"-->
						<field name="emiss_dom" sector="domestic"/>
						<field name="emiss_ind" sector="industry"/>
						<field name="emiss_tra" sector="traffic"/>
						<field name="emiss_wst" sector="wasteburning"/>
						<field name="emiss_awb" sector="agriculturalwaste"/>
		  			</fields>
			</inputfile>
			<inputfile 
	    	  			name="cmipemis/IPCC_emissions_SO2_ships_$year$_0.5x0.5_v1_20_04_2009.nc"
		  	  			provider="IPCC"
		      			year="1850,1860,1860,1870,1880,1890,1900,1910,1920,1930,1940,1950,1960,1970,1980,1990,2000"
						creationdate="20_04_2009 13_05_2009">
					<fields>
		  				<!-- possible fields are emissionLevel=n, output=override.nc"-->
						<field name="emiss_shp" sector="ships"/>
		  			</fields>
			</inputfile>
		</outputfile>

		<!-- HISTORICAL EVOLUTION OF SO2 BIOMASS BURNING EMISSIONS -->
		<outputfile
					name="IPCC_emis_so2bb_1850_2000.nc"
					field="emis_SO2_bb" 
					component="SO2"
					source = "bb"
					provider = "IPCC"
					scenario="historical">
			<inputfile
						name="cmipemis/IPCC_GriddedBiomassBurningEmissions_SO2_decadalmonthlymean$year$_v1.nc"
		  	  			provider="IPCC"
		      			year="1850,1860,1860,1870,1880,1890,1900,1910,1920,1930,1940,1950,1960,1970,1980,1990,2000">
				<fields>
		  			<!-- possible fields are emissionLevel=n, output=override.nc"-->
					<field name="grassfire" sector="domestic"/>
					<field name="forestfire" sector="energy"/>
		  		</fields>
			</inputfile>
		</outputfile>

		<!--RCP85 SCENARIO FOR SO2 Fossile Fuel -->
		<outputfile
					name="IPCC_emis_so2ff_RCP85.nc"
					field="emis_SO2_ff"
					component="SO2"
					source = "ff"
					provider = "IPCC"
					scenario="RCP85">
			<inputfile
						name="cmipemis/RCP85/IPCC_emissions_RCP85_SO2_anthropogenic_$year$_0.5x0.5_v1_21_12_2009.nc"
		  	  			provider="IPCC"
		      			year="2010,2020,2030,2040,2050,2060,2070,2080,2090,2100">
				<fields>
		  			<!-- possible fields are emissionLevel=n, output=override.nc"-->
					<field name="emiss_ene" sector="energy"/>
					<field name="emiss_dom" sector="domestic"/>
					<field name="emiss_ind" sector="industry"/>
					<field name="emiss_tra" sector="traffic"/>
					<field name="emiss_wst" sector="waste"/>
					<field name="emiss_agr" sector="agriculture"/>     <!--NOT IN HISTORICAL EMISIONS-->
					<field name="emiss_slv" sector="solvent"/>         <!--NOT IN HISTORICAL EMISSIONS-->
					<field name="emiss_awb" sector="agriculturWaste"/>
		  		</fields>
			</inputfile>
			<inputfile
						name="cmipemis/RCP85/IPCC_emissions_RCP85_SO2_ships_$year$_0.5x0.5_v1_21_12_2009.nc"
		  	  			provider="IPCC"
		      			year="2010,2020,2030,2040,2050,2060,2070,2080,2090,2100">
				<fields>
		  			<!-- possible fields are emissionLevel=n, output=override.nc"-->
					<field name="emiss_shp" sector="energy"/>
		  		</fields>
			</inputfile>
		</outputfile>


		<!-- RCP85 SCENARIO FOR SO2 Biomass burning   -->
		<outputfile
					name="IPCC_emis_so2bb_RCP85.nc"
					field="emis_SO2_bb"
					component="SO2"
					provider = "IPCC"
					source = "bb"
					scenario="RCP85">
			<inputfile
						name="cmipemis/RCP85/IPCC_emissions_RCP85_SO2_biomassburning_$year$_0.5x0.5_v1_21_12_2009.nc"
		  	  			provider="IPCC"
		      			year="2010,2020,2030,2040,2050,2060,2070,2080,2090,2100">
				<fields>
		  			<!-- possible fields are emissionLevel=n, output=override.nc"-->
					<field name="grassfire" sector="grassfire"/>
					<field name="forestfire" sector="forestfire"/>
		  		</fields>
			</inputfile>
		</outputfile>

  </outputfiles>
</CamOsloInputConfiguration>
